{
	"name": "svelte-games",
	"version": "0.1.5",
	"license": "GPL-3.0-only",
	"author": "DoodadLabs",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/doodad-labs/svelte-games.git"
	},
	"bugs": {
		"url": "https://github.com/doodad-labs/svelte-games/issues"
	},
	"homepage": "https://github.com/doodad-labs/svelte-games#readme",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build && npm run prepack",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"prepack": "svelte-kit sync && svelte-package && publint",
		"package": "svelte-package",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "eslint ."
	},
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*"
	],
	"sideEffects": [
		"**/*.css",
		"./dist/images/**/*"
	],
	"types": "./dist/index.d.ts",
	"type": "module",
	"exports": {
		"./Snap.svelte": {
			"types": "./dist/Snap.svelte.d.ts",
			"svelte": "./dist/Snap.svelte"
		},
		"./TicTacToe.svelte": {
			"types": "./dist/TicTacToe.svelte.d.ts",
			"svelte": "./dist/TicTacToe.svelte"
		},
		"./Snake.svelte": {
			"types": "./dist/Snake.svelte.d.ts",
			"svelte": "./dist/Snake.svelte"
		}
	},
	"typesVersions": {
		">4.0": {
			"Snap": [
				"./dist/Snap.d.ts"
			],
			"TicTacToe": [
				"./dist/TicTacToe.d.ts"
			],
			"Snake": [
				"./dist/Snake.d.ts"
			]
		}
	},
	"peerDependencies": {
		"svelte": "^5.0.0"
	},
	"devDependencies": {
		"@eslint/compat": "^1.2.5",
		"@eslint/js": "^9.18.0",
		"@sveltejs/adapter-auto": "^4.0.0",
		"@sveltejs/adapter-static": "^3.0.8",
		"@sveltejs/kit": "^2.16.0",
		"@sveltejs/package": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^5.0.0",
		"@tailwindcss/vite": "^4.0.0",
		"eslint": "^9.18.0",
		"eslint-plugin-svelte": "^3.0.0",
		"globals": "^16.0.0",
		"publint": "^0.3.2",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"tailwindcss": "^4.0.0",
		"typescript": "^5.0.0",
		"typescript-eslint": "^8.20.0",
		"vite": "^6.0.0"
	},
	"keywords": [
		"svelte",
		"component",
		"svelte-component",
		"games",
		"game-component",
		"loading-games",
		"loading-games-component",
		"snake",
		"snake-game",
		"tic-tac-toe",
		"tic-tac-toe-game"
	],
	"dependencies": {
		"esm-env": "^1.2.2"
	}
}
